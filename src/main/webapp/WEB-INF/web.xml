<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"         version="4.0">    <description>ATM System</description>    <welcome-file-list>        <welcome-file>index.jsp</welcome-file>        <welcome-file>index.html</welcome-file>        <welcome-file>index.htm</welcome-file>    </welcome-file-list>    <!-- 登录页面的映射 -->    <servlet>        <servlet-name>loginJsp</servlet-name>        <jsp-file>/WEB-INF/pages/login.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>loginJsp</servlet-name>        <url-pattern>/login</url-pattern>    </servlet-mapping>    <!-- 主界面的映射 -->    <servlet>        <servlet-name>selectJsp</servlet-name>        <jsp-file>/WEB-INF/pages/select.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>selectJsp</servlet-name>        <url-pattern>/select</url-pattern>    </servlet-mapping>    <!-- 存款界面的映射 -->    <servlet>        <servlet-name>depositConfirmJsp</servlet-name>        <jsp-file>/WEB-INF/pages/deposit/depositConfirm.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>depositConfirmJsp</servlet-name>        <url-pattern>/depositConfirm</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>depositEndJsp</servlet-name>        <jsp-file>/WEB-INF/pages/deposit/depositEnd.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>depositEndJsp</servlet-name>        <url-pattern>/depositEnd</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>depositFailJsp</servlet-name>        <jsp-file>/WEB-INF/pages/deposit/depositFail.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>depositFailJsp</servlet-name>        <url-pattern>/depositFail</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>depositInputJsp</servlet-name>        <jsp-file>/WEB-INF/pages/deposit/depositInput.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>depositInputJsp</servlet-name>        <url-pattern>/depositInput</url-pattern>    </servlet-mapping>    <!-- 转账界面的映射 -->    <servlet>        <servlet-name>transferConfirmJsp</servlet-name>        <jsp-file>/WEB-INF/pages/transfer/transferConfirm.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>transferConfirmJsp</servlet-name>        <url-pattern>/transferConfirm</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>transferInputJsp</servlet-name>        <jsp-file>/WEB-INF/pages/transfer/transferInput.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>transferInputJsp</servlet-name>        <url-pattern>/transferInput</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>transferFailJsp</servlet-name>        <jsp-file>/WEB-INF/pages/transfer/transferFail.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>transferFailJsp</servlet-name>        <url-pattern>/transferFail</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>transferEndJsp</servlet-name>        <jsp-file>/WEB-INF/pages/transfer/transferEnd.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>transferEndJsp</servlet-name>        <url-pattern>/transferEnd</url-pattern>    </servlet-mapping>    <!-- 取款界面的映射 -->    <servlet>        <servlet-name>withdrawInputJsp</servlet-name>        <jsp-file>/WEB-INF/pages/withdraw/withdrawInput.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>withdrawInputJsp</servlet-name>        <url-pattern>/withdrawInput</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>withdrawEndJsp</servlet-name>        <jsp-file>/WEB-INF/pages/withdraw/withdrawEnd.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>withdrawEndJsp</servlet-name>        <url-pattern>/withdrawEnd</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>withdrawFailJsp</servlet-name>        <jsp-file>/WEB-INF/pages/withdraw/withdrawFail.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>withdrawFailJsp</servlet-name>        <url-pattern>/withdrawFail</url-pattern>    </servlet-mapping>    <!-- 余额查询页面的映射 -->    <servlet>        <servlet-name>balanceQueryEndJsp</servlet-name>        <jsp-file>/WEB-INF/pages/balance/balanceQueryEnd.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>balanceQueryEndJsp</servlet-name>        <url-pattern>/balanceQueryEnd</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>balanceQueryFailJsp</servlet-name>        <jsp-file>/WEB-INF/pages/balance/balanceQueryFail.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>balanceQueryFailJsp</servlet-name>        <url-pattern>/balanceQueryFail</url-pattern>    </servlet-mapping>    <!-- 交易记录页面映射 -->    <servlet>        <servlet-name>recordCheckJsp</servlet-name>        <jsp-file>/WEB-INF/pages/recordCheck/recordCheck.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>recordCheckJsp</servlet-name>        <url-pattern>/recordCheck</url-pattern>    </servlet-mapping>    <!-- 修改账户信息页面映射 -->    <servlet>        <servlet-name>informationModifyJsp</servlet-name>        <jsp-file>/WEB-INF/pages/modify/informationModify.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>informationModifyJsp</servlet-name>        <url-pattern>/informationModify</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>modifyEndJsp</servlet-name>        <jsp-file>/WEB-INF/pages/modify/modifyEnd.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>modifyEndJsp</servlet-name>        <url-pattern>/modifyEnd</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>modifyFailJsp</servlet-name>        <jsp-file>/WEB-INF/pages/modify/modifyFail.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>modifyFailJsp</servlet-name>        <url-pattern>/modifyFail</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>modifySelectJsp</servlet-name>        <jsp-file>/WEB-INF/pages/modify/modifySelect.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>modifySelectJsp</servlet-name>        <url-pattern>/modifySelect</url-pattern>    </servlet-mapping>    <servlet>        <servlet-name>passwordChangeJsp</servlet-name>        <jsp-file>/WEB-INF/pages/modify/passwordChange.jsp</jsp-file>    </servlet>    <servlet-mapping>        <servlet-name>passwordChangeJsp</servlet-name>        <url-pattern>/passwordChange</url-pattern>    </servlet-mapping></web-app>